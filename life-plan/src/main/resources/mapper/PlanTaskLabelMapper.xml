<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.life.task.mapper.PlanTaskLabelMapper">

    <resultMap type="com.life.task.pojo.PlanTaskLabel" id="PlanTaskLabelResult">
        <result property="taskId" column="task_id"/>
        <result property="labelId" column="label_id"/>
        <result property="createTime" column="create_time"/>
        <result property="updateTime" column="update_time"/>
    </resultMap>

    <insert id="insertBatch"
            parameterType="java.util.List">
        insert into `plan_task_label`
        (`task_id`,`label_id`,`create_time`,`update_time`)
        values
        <foreach collection="list" item="item" index="index" separator=",">
            (
            #{planTaskLabel.taskId},
            #{planTaskLabel.labelId},
            #{planTaskLabel.createTime},
            #{planTaskLabel.updateTime}
            )
        </foreach>

    </insert>

</mapper>
